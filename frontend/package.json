{
  "name": "bizerta-rental-frontend",
  "version": "1.0.0",
  "description": "Plateforme de location de vacances Ã  Bizerte - Frontend React",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "react-scripts start",
    "build:prod": "react-scripts build && echo 'Build completed for production!'",
    "preview": "serve -s build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "rm -rf build node_modules package-lock.json && npm install",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001",
  "homepage": ".",
  "keywords": [
    "bizerte",
    "rental",
    "vacation",
    "tunisia",
    "react",
    "tailwind",
    "property",
    "booking"
  ],
  "author": {
    "name": "Bizerta Rental Team",
    "email": "contact@bizerta-rental.tn",
    "url": "https://bizerta-rental.tn"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/votre-username/bizerta-rental.git"
  },
  "bugs": {
    "url": "https://github.com/votre-username/bizerta-rental/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
